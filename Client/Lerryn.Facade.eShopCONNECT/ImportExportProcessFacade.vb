                    End If

                    If GetXMLElementText(m_ImportExportConfigFacade.SourceConfig, SOURCE_CONFIG_SET_DISABLE_FREIGHT_CALCULATION).ToUpper = "YES" Then ' TJS 17/03/09 TJS 07/06/09
