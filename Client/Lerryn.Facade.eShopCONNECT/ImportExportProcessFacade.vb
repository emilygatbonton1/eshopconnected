#Region " RoundDecimalValue "
    Public Function RoundDecimalValue(ByVal InputValue As Decimal, Optional ByVal DecimalPlacesRequired As Integer = 2) As Decimal
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
