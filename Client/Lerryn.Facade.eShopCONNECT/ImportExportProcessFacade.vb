                                        .TransactionTaxDetailView(0)("TaxValueIsFromSource_DEV000221") = True ' TJS 14/07/09 TJS 14/08/09
                                        .TransactionTaxDetailView(0).IsTAOverridden = True ' TJS 24/02/12
                                    Else
