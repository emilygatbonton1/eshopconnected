        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        Dim strFormat As String

        If DecimalPlacesRequired > 0 Then
            strFormat = "0" & ".0000000000".Substring(0, DecimalPlacesRequired + 1)
