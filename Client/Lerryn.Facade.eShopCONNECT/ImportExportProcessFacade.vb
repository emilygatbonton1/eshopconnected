                    If strShippingCompanyName <> "" Then ' TJS 08/02/09 
                        ' yes, add company name to address
                        NewCustomerDataset.CustomerShipToView(0).Address = strShippingCompanyName & ChrW(13) & ChrW(10) & strTempValue ' TJS 09/03/09
